{
  "name": "@skeet/gateway",
  "version": "2.6.0",
  "description": "Run MCP stdio servers over SSE or visa versa",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skeet-build/skeet-gateway.git"
  },
  "keywords": [
    "mcp",
    "stdio",
    "sse",
    "gateway",
    "proxy",
    "bridge"
  ],
  "type": "module",
  "bin": {
    "supergateway": "dist/index.js",
    "@skeet/gateway": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write 'src/**/*.ts' '*.json' '.prettierrc'",
    "format:check": "prettier --check 'src/**/*.ts' '*.json' '.prettierrc'",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.1",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.1",
    "yargs": "^17.7.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.0",
    "@types/ws": "^8.18.0",
    "@types/yargs": "^17.0.33",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
